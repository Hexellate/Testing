{
  "appId": "com.author.application",
  "productName": "Auto Updater Test",
  "generateUpdatesFilesForAllChannels": false,
  "detectUpdateChannel": false,
  "includePdb": false,
  "directories": {
    "output": "compiled"
  },
  "win": {
    "target": [
      {
        "target": "nsis-web",
        "arch": ["x64", "ia32"]
      },
      {
        "target": "nsis",
        "arch": ["x64", "ia32"]
      }
    ],
    "verifyUpdateCodeSignature": false
  },
  "nsis": {
    "oneClick": false,
    "perMachine": false,
    "allowElevation": true,
    "allowToChangeInstallationDirectory": true,
    "differentialPackage": true,
    "artifactName": "${productName} Setup ${version}.${ext}",
    "shortcutName": "Auto Updater Test"
  },
  "nsisWeb": {
    "artifactName": "${productName} Web Setup ${version}.${ext}"
  },
  "mac": {
    "category": "public.app-category.utility"
  },
  "dmg": {
    "contents": [
      {
        "x": 110,
        "y": 150
      },
      {
        "x": 240,
        "y": 150,
        "type": "link",
        "path": "/Applications"
      }
    ]
  },
  "linux": {
    "target": ["AppImage"]
  },
  "appImage": {
    "category": "Utility"
  },
  "publish": {
    "provider": "generic",
    "url": "Set at build time",
    "useMultipleRangeRequest": false
  }
}
