{
  "appId": "com.author.application.next",
  "productName": "Auto Updater Test - Next",
  "generateUpdatesFilesForAllChannels": false,
  "detectUpdateChannel": false,
  "includePdb": false,
  "directories": {
    "output": "compiled"
  },
  "win": {
    "target": [
      {
        "target": "nsis-web",
        "arch": ["x64", "ia32"]
      }
    ]
  },
  "nsis": {
    "oneClick": false,
    "perMachine": true,
    "allowElevation": true,
    "allowToChangeInstallationDirectory": true,
    "differentialPackage": true,
    "artifactName": "${productName} Setup ${version}.${ext}",
    "shortcutName": "Auto Updater Test - Next"
  },
  "mac": {
    "category": "public.app-category.utility"
  },
  "dmg": {
    "contents": [
      {
        "x": 110,
        "y": 150
      },
      {
        "x": 240,
        "y": 150,
        "type": "link",
        "path": "/Applications"
      }
    ]
  },
  "linux": {
    "target": ["AppImage"]
  },
  "appImage": {
    "category": "Utility"
  },
  "publish": {
    "provider": "generic",
    "url": "https://raw.githubusercontent.com/Hexellate/tracking/master/autoupdate-test/updates/next"
  }
}
