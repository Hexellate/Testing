{
  "name": "autoupdate-test",
  "productName": "Auto Update Test",
  "version": "0.5.0-canary.273",
  "author": {
    "name": "Daniel Nichols",
    "email": "info.danielnichols@gmail.com"
  },
  "description": "update testing",
  "main": "dist/main/main.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hexellate/Electron-React-Template.git"
  },
  "scripts": {
    "dev": "npm-run-all --parallel webpack-dev react-devtools",
    "webpack-dev": "electron-webpack dev --remote-debugging-port=9223",
    "react-devtools": "react-devtools",
    "pack": "electron-webpack",
    "build": "babel-node scripts/build.js",
    "clean": "rimraf \"dist\" && rimraf \"compiled\"",
    "clean-npm": "rimraf \"package-lock.json\" && rimraf \"node_modules\"",
    "rebuild-npm": "npm run clean-npm && npm install",
    "clean-all": "npm run clean && npm run clean-npm",
    "test": "echo no tests set up"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "electron": "^3.0.10",
    "electron-builder": "^20.38.1",
    "electron-webpack": "^2.6.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.0",
    "husky": "^1.2.0",
    "js-yaml": "^3.12.0",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.10.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.2",
    "react": "^16.6.3",
    "react-devtools": "^3.4.3",
    "react-dom": "^16.6.3",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.9.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.5.1",
    "terser-webpack-plugin": "^1.1.0",
    "webpack": "^4.26.1"
  },
  "dependencies": {
    "electron-log": "^2.2.17",
    "electron-updater": "^4.0.5",
    "lodash": "^4.17.11",
    "minimist": "^1.2.0",
    "prop-types": "^15.6.2",
    "redux": "^4.0.1",
    "semver": "^5.6.0",
    "source-map-support": "^0.5.5",
    "styled-components": "^4.1.2"
  },
  "electronWebpack": {
    "renderer": {
      "webpackConfig": "webpack.renderer.additions.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js, jsx, json}": [
      "eslint --fix",
      "git add"
    ],
    "*.{css, scss, md, html, yml, yaml}": [
      "prettier --write",
      "git add"
    ]
  }
}
