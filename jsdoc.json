{
  "source": {
    "include": [
      "src"
    ],
    "exclude": [],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "opts": {
    "template": "node_modules/tui-jsdoc-template",
    "recurse": true,
    "destination": "./docs/"
  },
  "templates": {
    "name": "autoupdate-test docs",
    "footerText": "<b>Copyright &copy;</b> 2018-2019 Daniel Nichols",
    "logo": {
      "width": "0px",
      "height": "0px"
    }
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  }
}
